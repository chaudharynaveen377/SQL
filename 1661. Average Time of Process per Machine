# Write your MySQL query statement below
select ac1.machine_id ,  ROUND(AVG(ac2.timestamp - ac1.timestamp), 3) AS processing_time from Activity as ac1 inner join Activity ac2 on ac1.machine_id = ac2.machine_id 

where ac1.machine_id = ac2.machine_id and ac1.process_id = ac2.process_id and ac1.activity_type ="start" and ac2.activity_type ="end"

group by ac1.machine_id
order by ac1.machine_id asc
